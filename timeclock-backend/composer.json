{
    "name": "timeclock-backend",
    "description": "The API backend for time clock project",
    "keywords": ["yii2", "rest api", "time clock"],
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "2.0.10",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-jui": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "vlucas/phpdotenv": "1.0.*",
        "codemix/yii2-streamlog": "0.0.1"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-codeception": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0"
    },
    "config": {
        "process-timeout": 1800,
        "vendor-dir": "/var/www/vendor"
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "/var/www/vendor/npm",
            "bower-asset-library": "/var/www/vendor/bower"
        }
    }
}
